    <button type="button" class="btn btn-primary launch" data-toggle="modal" data-target="#staticBackdrop">
        <i class="fa fa-rocket"></i> Pay Now
    </button>
    <!-- Modal -->
    <!DOCTYPE html>
<html>
   <head>
      <!-- Basic -->
      <meta charset="utf-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge" />
